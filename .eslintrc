{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["love", "plugin:react/recommended", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "plugins": ["@typescript-eslint", "react"],
  "rules": {
    "@typescript-eslint/non-nullable-type-assertion-style": "off"
  }
}
